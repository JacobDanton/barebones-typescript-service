{
    "version": "1.4.1",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": false,
        "noLib": false,
        "outDir": "build"
    },
    "filesGlob": [
        "gulpfile.ts",
        "./src/**/*.ts",
        "./test/**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "gulpfile.ts",
        "./src/app.ts",
        "./src/configuration.ts",
        "./src/errors.ts",
        "./src/server.ts",
        "./src/storage/memoryNameStorage.ts",
        "./src/storage/nameStorage.ts",
        "./src/user.ts",
        "./test/serverTest.ts"
    ]
}
